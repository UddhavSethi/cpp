#include <iostream>
using namespace std;
int main()
{
    int i,cost[3]={1,2,3};
    for(i=0;i<3;i++){
        cout<<cost[i]<<" ";
    }
}
#in cpp unlike in python you cant directly print array, that would print memory location of the first element in the array instead you use for loop to iterate through all itenms and print them one by one
